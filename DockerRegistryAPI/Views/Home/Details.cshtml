@{
    ViewBag.Title = "Image Repository";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<DockerRegistryAPI.Models.ImageModel>
<h2>Image Repository</h2>

<table class="table">
    <thead><tr><td style="font-weight: bold">Details</td></tr></thead>
    <tbody>
        @foreach (var Image in this.Model)
        {
            <tr>
                <td>@Image.Architecture</td>
            </tr>
        }
    </tbody>
</table>
